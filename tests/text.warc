window.Counted = false;

jQuery(document).foundation('joyride', 'start');

});

jQuery(window).on('DOMContentLoaded load resize scroll', function() {
  if(window.Counted==false){
        var scroll = jQuery(window).scrollTop();
        var height = jQuery(window).height();
        if(scroll + height > jQuery('.count-up').offset().top){

    jQuery('.count-up').each(function(){
       // For each COUNTER we have to initialize some variables
      var me = jQuery(this);
      var max = parseInt(me.text());
      var interval = max/100;
      var value = interval;
      me.text(Math.floor(interval));
  
      var loop = setInterval(function(){
         value += interval;
         if(value >= max){  
            value = max; 
           clearInterval(loop);
        }
        me.text(Math.floor(value));
    }, 20); 
   });

   jQuery('.count-down').each(function(){
     var me = jQuery(this);
     var max = parseInt(me.attr('data-max'));
     var target = parseInt(me.text());
     var interval = (max-target)/100;
     var value = max;
     me.text(value);

     var loop = setInterval(function(){
           value -= interval;
           if(value < target){
              value = target;
              clearInterval(loop);
            }
            me.text(Math.floor(value));
       }, 20);
});

   jQuery('.year-count-down').each(function(){
      var me = jQuery(this);
      var now = new Date();
      var max = parseInt(now.getFullYear());
      var target = parseInt(me.text());
      var interval = (max-target)/100;
      var value = max;
      me.text(Math.floor(value));
  
      var loop = setInterval(function(){
         value -= interval;
         if(value <= target){  
            value = target; 
           clearInterval(loop);
        }
        me.text(Math.floor(value));
    }, 20); 
   });

  window.Counted=true;
}
}
});

